	        "on":          {"@type":"@id", "@id":"oa:hasTarget"},
	        "full":        {"@type":"@id", "@id":"oa:hasSource"},
	        "selector":    {"@type":"@id", "@id":"oa:hasSelector"},
	        "stylesheet":  {"@type":"@id", "@id":"oa:styledBy"},
	        "style":       {"@id":"oa:styleClass"},

		    "default":       {"@type":"@id", "@id" : "oa:default"},
		    "item":          {"@type":"@id", "@id" : "oa:item"},
		    "chars":        "cnt:chars",
