    },

    "Application": "as:Application",
    "Dataset": "dctypes:Dataset",
    "Image": "dctypes:StillImage",
    "Video": "dctypes:MovingImage",
    "Audio": "dctypes:Sound",
    "Text": "dctypes:Text",
    "Service": "svcs:Service",

    "ImageService1": {
      "@id": "http://iiif.io/api/image/1/ImageService",
