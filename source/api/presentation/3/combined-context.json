      },
      "AnnotationPage": {
        "@id": "as:OrderedCollectionPage",
        "@context": "http://www.w3.org/ns/anno.jsonld"
      },
      "Annotation": {
        "@id": "oa:Annotation",
        "@context": "http://www.w3.org/ns/anno.jsonld"
      },
