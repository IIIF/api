      },
      "sizeAboveFull": {
        "@id": "image:sizeAboveFullFeature",
        "@type": "image:Feature"
      },
      "sizeByForcedWh": {
        "@id": "image:sizeByForcedWHFeature",
        "@type": "image:Feature"
      },
      "sizeByH": {
        "@id": "image:sizeByHFeature",
        "@type": "image:Feature"
      },
      "sizeByPct": {
        "@id": "image:sizeByPctFeature",
        "@type": "image:Feature"
      },
      "sizeByW": {
        "@id": "image:sizeByWFeature",
        "@type": "image:Feature"
      },
      "sizeByWh": {
        "@id": "image:sizeByWHFeature",
        "@type": "image:Feature"
      },
      "sizeByWhListed": {
        "@id": "image:sizeByWHListedFeature",
        "@type": "image:Feature"
      },
      "sizeByConfinedWh": {
        "@id": "image:sizeByConfinedWHFeature",
