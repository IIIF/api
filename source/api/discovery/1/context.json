{
  "@context": [
  	"https://www.w3.org/ns/activitystreams",
  	{
      "as": "https://www.w3.org/ns/activitystreams#",
      "dcterms": "http://purl.org/dc/terms/",
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
      "discov": "https://iiif.io/api/discovery/1#",
      "oa": "http://www.w3.org/ns/oa#",

      "OrderedCollection": {
        "@id": "as:OrderedCollection",
        "@context": {
          "rights": {
            "@id": "dcterms:rights",
            "@type": "@id"
          },
          "partOf": {
            "@id": "dcterms:partOf",
            "@type": "@id"
          }
        }
      },

      "Refresh": {
        "@id": "discov:Refresh",
        "@context": {
          "partOf": {
            "@id": "dcterms:partOf",
            "@type": "@id"
          }
        }        
      },

      "Create": {
        "@id": "as:Create",
        "@context": {
          "partOf": {
            "@id": "dcterms:partOf",
            "@type": "@id"
          }
        }
      },

      "Update": {
        "@id": "as:Update",
        "@context": {
          "partOf": {
            "@id": "dcterms:partOf",
            "@type": "@id"
          }
        }
      },

      "Delete": {
        "@id": "as:Delete",
        "@context": {
          "partOf": {
            "@id": "dcterms:partOf",
            "@type": "@id"
          }
        }
      },

      "Add": {
        "@id": "as:Delete",
        "@context": {
          "partOf": {
            "@id": "dcterms:partOf",
            "@type": "@id"
          }
        }
      },

      "Remove": {
        "@id": "as:Delete",
        "@context": {
          "partOf": {
            "@id": "dcterms:partOf",
            "@type": "@id"
          }
        }
      },
      "Move": {
        "@id": "as:Delete",
        "@context": {
          "partOf": {
            "@id": "dcterms:partOf",
            "@type": "@id"
          }
        }
      },


      "label": {
        "@id": "rdfs:label",
        "@container": ["@language", "@set"],
        "@context": {
          "none": "@none"
        }
      },    
      "seeAlso": {
        "@type": "@id",
        "@id": "rdfs:seeAlso",
        "@container": "@set"
      },

      "format": {
        "@id": "dc:format"
      },
      "profile": {
        "@type": "@vocab",
        "@id": "dcterms:conformsTo"
      },
      "canonical": {
        "@type": "@id",
        "@id": "oa:canonical"
      },
      "provider": {
        "@type": "@id",
        "@id": "schema:provider",
        "@container": "@set"
      },
      "Agent": "dcterms:Agent",

	    "object": {
  		  "@id": "as:object",
  		  "@type": "@id",
  		  "@context": {
	        "prezi": "http://iiif.io/api/presentation/3#",
	        "img": "http://iiif.io/api/image/3#",
  		    "oa": "http://www.w3.org/ns/oa#",
		      "dctypes": "http://purl.org/dc/dcmitype/",

	  	    "Collection": "prezi:Collection",
	  	    "Manifest": "prezi:Manifest",
	  	    "Range": "prezi:Range",
	  	    "Canvas": "prezi:Canvas",
	  	    "Annotation": "oa:Annotation",
	  	    "AnnotationPage": "as:OrderedCollectionPage",
	  	    "AnnotationCollection": "as:OrderedCollection",

          "Dataset": "dctypes:Dataset",
          "Image": "dctypes:StillImage",
          "Video": "dctypes:MovingImage",
          "Audio": "dctypes:Sound",
          "Text": "dctypes:Text",
          "ImageService3": "img:ImageService",
          "ImageService2": "http://iiif.io/api/image/2#ImageService"
		    }
	    },
      "target": {
        "@id": "as:target",
        "@type": "@id",
        "@context": {
          "prezi": "http://iiif.io/api/presentation/3#",
          "img": "http://iiif.io/api/image/3#",
          "oa": "http://www.w3.org/ns/oa#",
          "dctypes": "http://purl.org/dc/dcmitype/",

          "Collection": "prezi:Collection",
          "Manifest": "prezi:Manifest",
          "Range": "prezi:Range",
          "Canvas": "prezi:Canvas",
          "Annotation": "oa:Annotation",
          "AnnotationPage": "as:OrderedCollectionPage",
          "AnnotationCollection": "as:OrderedCollection",

          "Dataset": "dctypes:Dataset",
          "Image": "dctypes:StillImage",
          "Video": "dctypes:MovingImage",
          "Audio": "dctypes:Sound",
          "Text": "dctypes:Text",
          "ImageService3": "img:ImageService",
          "ImageService2": "http://iiif.io/api/image/2#ImageService"
        }
      }



  	}
  ]
}
