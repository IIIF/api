{
   "@context":[
      "https://geojson.org/geojson-ld/geojson-context.jsonld",
      "http://www.w3.org/ns/anno.jsonld",
      {
         "@version":1.1,
         "xsd": "http://www.w3.org/2001/XMLSchema#",
         "iiif_georef":"http://iiif.io/api/extension/georef/vocab/georef-terms/#",
         "iiif_selectors":"http://iiif.io/api/registry/selectors#",
         "georeferencing":"iiif_georef:georeferencing",
         "polynomial":"iiif_georef:polynomial",
         "thinPlateSpline":"iiif_georef:thinPlateSpline",
         "id":"@id",
         "type":"@type",
         "ImageApiSelector":{
            "@id":"iiif_selectors:ImageApiSelector",
            "@context":[
               {
                  "iiif_image":"http://iiif.io/api/image/#",
                  "region":{
                     "@id":"iiif_image:regionValue",
                     "@type":"xsd:string"
                  },
                  "size":{
                     "@id":"iiif_image:sizeValue",
                     "@type":"xsd:string"
                  },
                  "rotation":{
                     "@id":"iiif_image:rotationValue",
                     "@type":"xsd:string"
                  },
                  "format":{
                     "@id":"iiif_image:formatValue",
                     "@type":"xsd:string"
                  },
                  "quality":{
                     "@id":"iiif_image:qualityValue",
                     "@type":"xsd:string"
                  }
               }
            ]
         },
         "transformation":{
            "@id":"iiif_georef:transformation",
            "@context":{
               "options":{
                  "@id":"iiif_georef:options",
                  "@context":{
                     "order":{
                        "@id":"iiif_georef:order",
                        "@type":"xsd:integer"
                     }
                  }
               }
            }
         },
         "resourceCoords":{
            "@id":"iiif_georef:resourceCoords",
            "@container":"@list",
            "@type":"xsd:float"
         }
      }
   ]
}