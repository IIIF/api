---
---
{
  "@context": {
    "iiif" :   "http://iiif.io/api/image/{{ site.image_api.latest.major }}#",
    "xsd" :    "http://www.w3.org/2001/XMLSchema#",
    "exif":    "http://www.w3.org/2003/12/exif/ns#",
    "dc":      "http://purl.org/dc/elements/1.1/",
    "dcterms": "http://purl.org/dc/terms/",
    "doap" :   "http://usefulinc.com/ns/doap#",

    "cors" : { "@id" : "iiif:corsFeature" },
    "region_by_pct" : { "@id" : "iiif:regionByPctFeature" },
    "region_by_px" : { "@id" : "iiif:regionByPxFeature" },
    "rotation_arbitrary" : { "@id" : "iiif:arbitraryRotationFeature" },
    "rotation_by_90s" : { "@id" : "iiif:rotationBy90sFeature" },
    "size_above_full" : { "@id" : "iiif:sizeAboveFullFeature" },
    "size_by_forced_wh" : { "@id" : "iiif:sizeByForcedWHFeature" },
    "size_by_h" : { "@id" : "iiif:sizeByHFeature" },
    "size_by_pct" : { "@id" : "iiif:sizeByPctFeature" },
    "size_by_w" : { "@id" : "iiif:sizeByWFeature" },
    "size_by_wh" : { "@id" : "iiif:sizeByWHFeature" },
    "profile_link_header" : {"@id" : "iiif:profileLinkHeaderFeature"},
    "canonical_link_header" : {"@id" : "iiif:canonicalLinkHeaderFeature"},
    "jsonld_media_type" : {"@id" : "iiif:jsonLdMediaTypeFeature"},

    "height":  { "@id" : "exif:height", "@type":"xsd:integer" },
    "width": { "@id" : "exif:width", "@type":"xsd:integer" },
    "tile_height" : { "@id" : "iiif:tileHeight", "@type" : "xsd:integer" },
    "tile_width" : { "@id" : "iiif:tileWidth", "@type" : "xsd:integer" },
    "profile" : { "@id" : "dcterms:conformsTo", "@type" : "iiif:ImageProfile", "@container" : "@list" },
    "protocol" : { "@id" : "doap:implements", "@type" : "@id" },
    "formats" : { "@id" : "iiif:format", "@type" : "xsd:string" },
    "qualities" : { "@id" : "iiif:quality", "@type" : "xsd:string" },
    "scale_factors" : { "@id" : "iiif:scaleFactor", "@type" : "xsd:string" },
    "sizes" : { "@id" : "iiif:size", "@type" : "xsd:string" },
    "supports" : { "@id" : "iiif:supports", "@type" : "@id" }

  }
}
